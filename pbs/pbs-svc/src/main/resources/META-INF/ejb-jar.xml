<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar version="3.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/ejb-jar_3_2.xsd">
	<enterprise-beans>
		<session>
			<ejb-name>WebSvcsJobSvc</ejb-name>
			<env-entry>
				<env-entry-name>domain</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>websvcs.osc.edu</env-entry-value>
			</env-entry>
			<env-entry>
				<env-entry-name>path</env-entry-name>
				<env-entry-type>java.lang.String</env-entry-type>
				<env-entry-value>/pbsacct/jobinfo.php</env-entry-value>
			</env-entry>
		</session>
	</enterprise-beans>
</ejb-jar>
